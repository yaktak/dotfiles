# This is Git's per-user configuration file.
[user]
    name = yaktak
    email = yakei034@gmail.com
[push]
    default = current
[core]
    excludesfile = ~/.gitignore
    editor = vim
[color]
    diff = auto
    status = auto
    branch = auto
[alias]
    a = add
    b = branch
    ba = branch --all
    bd = branch --delete
    bm = branch --move
    co = checkout
    cob = checkout -b
    c = commit
    ca = commit --all
    can = commit --all --amend
    cam = commit --all --message
    camn = commit --all --message --amend
    cm = commit --message
    cmn = commit --message --amend
    cn = commit --amend
    d = diff
    dc = diff --cached
    dw = diff --word-diff
    dwc = diff --word-diff --cached
    f = fetch
    m = merge
    mt = mergetool -t vimdiff2
    p = push
    pu = push --set-upstream
    puf = push --set-upstream --force
    pf = push --force
    pl = pull
    plr = pull --rebase
    pwb = symbolic-ref --short HEAD
    rb = rebase
    rbi = rebase --interactive
    rbc = rebase --continue
    rss = reset --soft
    rsm = reset --mixed
    rsh = reset --hard
    sa = stash apply
    sc = stash clear
    ss = stash save
    ssu = stash save -u
    sl = stash list
    sp = stash pop
    s = status
    l = log
    lo = log --oneline
    lgo = log --graph --oneline --pretty=format:'%C(yellow)%h%Creset -%C(red)%d%Creset %s %C(bold blue)<%an>%Creset'
[http]
    sslVerify = false
