# This is Git's per-user configuration file.
[user]
	name = yaktak
	email = yakei034@gmail.com
[push]
	default = simple
[core]
    excludesfile = ~/.gitignore
    editor = vim
[color]
    diff = auto
    status = auto
    branch = auto
[alias]
    a = add
    b = branch
    ba = branch -a
    bd = branch -d
    bm = branch -m
    bD = branch -D
    co = checkout
    cob = checkout -b
    c = commit
    ca = commit --all
    cA = commit --amend
    cm = commit -m
    cam = commit -am
    d = diff
    dc = diff --cached
    dw = diff --word-diff
    dwc = diff --word-diff --cached
    f = fetch
    m = merge
    mt = mergetool -t vimdiff2
    p = push
    pu = push --set-upstream
    puf = push --set-upstream --force
    pf = push --force
    rb = rebase
    rbi = rebase -i
    rsh = reset --mixed HEAD
    rssh = reset --soft HEAD
    rsmh = reset --mixed HEAD
    rshh = reset --hard HEAD
    sa = stash apply
    sc = stash clear
    ss = stash save
    ssu = stash save -u
    sl = stash list
    sp = stash pop
    s = status
    l = log
    l1 = log -1
    lo = log --oneline
    lo1 = log --oneline -1
